if [ "$1" = "1" ] 
then

./exsm -m ../data/tipton.mesh << EOF
0 2 1 1 1 2 400 200 1
EOF

elif [ "$1" = "2" ]
then
# this is a commented out line
./exsm -m ../data/tipton.mesh << EOF
0 2 2 9 1 2 400 200 1
EOF

elif [ "$1" = "3" ]
then
# this is a commented out line
./exsm -m ../data/tipton.mesh << EOF
0 2 3 9 1 2 400 200 1
EOF

elif [ "$1" = "4" ]
then
# this is a commented out line
./exsm -m ../data/tipton.mesh << EOF
0 2 1 1 1 2 400 200 1
EOF
fi

